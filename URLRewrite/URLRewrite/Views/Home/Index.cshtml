@{
    ViewBag.Title = "Home Page";
}

@model string[]

<br />
<div class="container">
    <div class="row">
        @foreach (var item in Model)
        {
            if (item.Contains("Redirec"))
            {
                <div class="col-md-12"><span class="label label-primary">@item</span></div>
            }
            else
            {
                <div class="col-md-12"><span class="label label-default">@item</span></div>
            }
        }
    </div>
</div>